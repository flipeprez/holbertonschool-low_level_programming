#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
	int n;
	ld n%10;
	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (ld < 5)
	printf("and is greater than 5");
	if (ld == 0)
	printf("and is 0");
	if (ld < 6)
	printf("and is less than 6 and not 0");
	printf("Last digit of %d is", n, ld);
	return (0);
}
